<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.LoginViewController">
   <children>
      <Button fx:id="loginButton" layoutX="200.0" layoutY="236.0" mnemonicParsing="false" onAction="#processLogin" prefHeight="25.0" prefWidth="58.0" text="Login" />
      <Button fx:id="signUpButton" layoutX="47.0" layoutY="236.0" mnemonicParsing="false" onAction="#processSignUp" text="Sign Up" />
      <TextField fx:id="usernameField" layoutX="76.0" layoutY="98.0" promptText="username" />
      <PasswordField fx:id="passwordField" layoutX="76.0" layoutY="150.0" promptText="password" />
      <Label fx:id="loginStatus" layoutX="37.0" layoutY="58.0" prefHeight="25.0" prefWidth="209.0" text="Status" textFill="#e10f0f">
         <font>
            <Font size="10.0" />
         </font></Label>
      <Label layoutX="33.0" layoutY="14.0" prefHeight="35.0" prefWidth="209.0" text="Maze Solver Game" textFill="#420bd9">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
